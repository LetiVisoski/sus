<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.sus.HelloController">
   <children>
      <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Label fx:id="atualizarPessoaLabel" layoutX="229.0" layoutY="26.0" text="Atualizar Pessoas">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <Label fx:id="idpessoaLabel" layoutX="14.0" layoutY="79.0" text="Digite o ID da pessoa que deseja atulalizar :">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <TextField fx:id="idpessoaTextField" layoutX="310.0" layoutY="77.0" />
            <Button layoutX="355.0" layoutY="114.0" mnemonicParsing="false" onAction="#buscarPessoa" text="Buscar">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Label fx:id="nomeLabel" layoutX="14.0" layoutY="163.0" text="Nome :">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label fx:id="cpfLabel" layoutX="14.0" layoutY="220.0" prefHeight="0.0" prefWidth="68.0" text="CPF/ CNPJ:">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label fx:id="nascimentoLabel" layoutX="13.0" layoutY="270.0" text="Data de Nascimento :">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label fx:id="telefoneLabel" layoutX="18.0" layoutY="312.0" text="Telefone :">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <TextField fx:id="nomeTextField" layoutX="171.0" layoutY="161.0" />
            <TextField fx:id="cpfTextField" layoutX="171.0" layoutY="218.0" />
            <TextField fx:id="nascimentoTextField" layoutX="171.0" layoutY="268.0" />
            <TextField fx:id="telefoneTextField" layoutX="171.0" layoutY="310.0" />
            <Button layoutX="198.0" layoutY="345.0" mnemonicParsing="false" onAction="#atualizarPessoas" text="Atualizar">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button layoutX="20.0" layoutY="20.0" mnemonicParsing="false" onAction="#voltar" text="Voltar">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </children>
</VBox>
