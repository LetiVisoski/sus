<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="org.example.sus.ListarViagemController">
    <children>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
            <children>
                <!-- Botão Voltar -->
                <Button fx:id="voltarButton" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#voltar" text="Voltar"/>

                <!-- Tabela de Viagens -->
                <TableView fx:id="tableView" layoutX="-2.0" layoutY="50.0" prefHeight="300.0" prefWidth="640.0">
                    <columns>
                        <TableColumn fx:id="id_viagemColumn" prefWidth="110.0" text="Id Viagem"/>
                        <TableColumn fx:id="dataColumn" prefWidth="86.0" text="Data"/>
                        <TableColumn fx:id="destinoColumn" prefWidth="135.0" text="Destino"/>
                        <TableColumn fx:id="horaColumn" prefWidth="67.0" text="Hora"/>
                        <TableColumn fx:id="motoristaColumn" prefWidth="70.0" text="Motorista"/>
                        <TableColumn fx:id="veiculoColumn" prefWidth="78.0" text="Veículo"/>
                        <TableColumn fx:id="embarqueColumn" prefWidth="110.0" text="Embarque"/>
                    </columns>
                </TableView>
            </children>
        </AnchorPane>
    </children>
</VBox>
