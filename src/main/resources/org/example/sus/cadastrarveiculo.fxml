<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.sus.HelloController">
   <children>
      <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <!-- Botão Voltar -->
            <Button fx:id="voltarCadastroVeiculo" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#voltarVeiculo" text="Voltar" />

            <!-- Título -->
            <Label fx:id="cadastroLabel" layoutX="199.0" layoutY="24.0" prefHeight="35.0" prefWidth="228.0" text="Cadastro de Veículo">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>

            <!-- Labels e Campos de Texto -->
            <Label fx:id="modeloLabel" layoutX="22.0" layoutY="116.0" text="Modelo :">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label fx:id="placaLabel" layoutX="22.0" layoutY="187.0" text="Placa :">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label fx:id="lugarLabel" layoutX="22.0" layoutY="251.0" text="Lugares :">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>

            <!-- Campos de Texto -->
            <TextField fx:id="tipoTextField" layoutX="148.0" layoutY="117.0" />
            <TextField fx:id="placaTextField" layoutX="148.0" layoutY="188.0" />
            <TextField fx:id="lugarTextField" layoutX="148.0" layoutY="252.0" />

            <!-- Botão Adicionar -->
            <Button layoutX="184.0" layoutY="327.0" mnemonicParsing="false" onAction="#AdicionarVeiculo" text="Adicionar">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </children>
</VBox>
