<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.sus.HelloController">
    <children>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
            <children>
                <!-- Botão Voltar -->
                <Button fx:id="voltarViagem" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#voltarViagem" text="Voltar" />

                <!-- Título -->
                <Label fx:id="viagemLabel" layoutX="241.0" layoutY="23.0" text="Excluir Viagens">
                    <font>
                        <Font size="24.0" />
                    </font>
                </Label>

                <!-- Labels e Campos de Texto -->
                <Label fx:id="idviagemLabel" layoutX="14.0" layoutY="85.0" text="Digite o Id da viagem que deseja excluir :">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Label>
                <TextField fx:id="idviagemTextField" layoutX="399.0" layoutY="86.0" />

                <!-- Botão Excluir -->
                <Button layoutX="283.0" layoutY="149.0" mnemonicParsing="false" onAction="#excluirViagem" text="Excluir">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Button>
            </children>
        </AnchorPane>
    </children>
</VBox>
